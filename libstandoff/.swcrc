{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "target": "es2019",
        "keepClassNames": true,
        "loose": true
    },
    "module": {
        "type": "commonjs",
        "strict": true,
        "noInterop": true
    },
    "sourceMaps": true
}
